{
  "name": "destack-workspace",
  "version": "1.0.0",
  "workspaces": [
    "./lib/*",
    "./dev/*"
  ],
  "scripts": {
    "start": "concurrently --kill-others \"cd lib/destack && npm run dev\" \"cd dev/nextjs-project && npm run dev\"",
    "prepare": "husky install"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "concurrently": "^6.2.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.3.2"
  }
}
